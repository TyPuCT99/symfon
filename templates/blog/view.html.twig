{% extends "blog/list.html.twig" %}
{% block content%}
    <table>
        <tr>
            <td>Title</td>
            <td>Description</td>
        </tr>
        {% for post in posts %}

            <tr>
                <td><a href="{{ url('showpost',{'id':post.id}) }}">{{ post.title }}  </td>
                <td>{{ post.description }}</td>
            </tr>
        {% endfor %}
    </table>
{% endblock %}